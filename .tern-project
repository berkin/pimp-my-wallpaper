{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js",
	"src/**/*.jsx"
  ],
  "dontLoad": [
    "node_modules/**",
    "dist",
	"lib",
	"coverage",
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
